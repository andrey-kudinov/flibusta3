  "bugs": {
    "url": "https://github.com/krys2fa/vanilla-js-starter-pack/issues"
  },
  "homepage": "https://github.com/krys2fa/vanilla-js-starter-pack#readme",
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.20.2",
    "html-webpack-plugin": "4.4.1",
    "stylelint": "13.3.3",
    "stylelint-config-standard": "20.0.0",
    "stylelint-csstree-validator": "1.8.0",
    "stylelint-scss": "3.17.2",
    "webpack": "4.44.1",
